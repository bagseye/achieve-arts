{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bwp/testimonials-item",
	"version": "0.1.0",
	"title": "Testimonials Item",
	"category": "widgets",
	"icon": "awards",
	"description": "A single testimonial that is only available in the Testimonials block.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"name": {
			"type": "string",
			"default": ""
		},
		"role": {
			"type": "string",
			"default": ""
		},
		"mediaid": {
			"type": "number",
			"default": null
		},
		"mediasrc": {
			"type": "string",
			"default": ""
		},
		"mediaalt": {
			"type": "string",
			"default": ""
		},
		"variant": {
			"type": "string",
			"default": "testimonials-default"
		}
	},
	"usesContext": [ "bwp/testimonialscount", "bwp/testimonialsvariant" ],
	"parent": [ "bwp/testimonials" ],
	"textdomain": "testimonials-item",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
