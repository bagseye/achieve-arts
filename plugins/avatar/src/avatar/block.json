{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bwp/avatar",
	"version": "0.1.0",
	"title": "Avatar",
	"category": "widgets",
	"icon": "admin-users",
	"description": "Displays a avatar with name and details.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"name": {
			"type": "string",
			"default": ""
		},
		"role": {
			"type": "string",
			"default": ""
		},
		"mediaId": {
			"type": "number",
			"default": 0
		},
		"mediaUrl": {
			"type": "string",
			"default": ""
		},
		"mediaAlt": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "avatar",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
