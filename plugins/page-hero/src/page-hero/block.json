{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bwp/page-hero",
	"version": "0.1.0",
	"title": "Page Hero",
	"category": "widgets",
	"icon": "shield",
	"description": "Displays a large hero area for use on pages.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"heading": {
			"type": "string",
			"default": ""
		},
		"tab": {
			"type": "string",
			"default": ""
		},
		"mediaId": {
			"type": "number",
			"default": 0
		},
		"mediaUrl": {
			"type": "string",
			"default": ""
		},
		"mediaAlt": {
			"type": "string",
			"default": ""
		},
		"videoid": {
			"type": "number",
			"default": 0
		},
		"videosrc": {
			"type": "string",
			"default": ""
		},
		"topmargin": {
			"type": "boolean",
			"default": false
		},
		"bottommargin": {
			"type": "boolean",
			"default": false
		},
		"altlayout": {
			"type": "boolean",
			"default": false
		},
		"includegradient": {
			"type": "boolean",
			"default": false
		},
		"bgcolour": {
			"type": "string",
			"default": "dark"
		},
		"variant": {
			"type": "string",
			"default": "page-hero-default"
		},
		"mediatype": {
			"type": "string",
			"default": "image"
		}
	},
	"textdomain": "page-hero",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"variations": "file:./variations.php"
}
