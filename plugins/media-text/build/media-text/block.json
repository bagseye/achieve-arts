{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "bwp/media-text",
  "version": "0.1.0",
  "title": "Media & Text",
  "category": "widgets",
  "icon": "id",
  "description": "Displays media and text content with alternate layouts.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "mapLat": {
      "type": "string",
      "default": ""
    },
    "mapLong": {
      "type": "string",
      "default": ""
    },
    "mapUrl": {
      "type": "string",
      "default": ""
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "tab": {
      "type": "string",
      "default": ""
    },
    "mediaId": {
      "type": "number",
      "default": 0
    },
    "mediaUrl": {
      "type": "string",
      "default": ""
    },
    "mediaAlt": {
      "type": "string",
      "default": ""
    },
    "topmargin": {
      "type": "boolean",
      "default": false
    },
    "bottommargin": {
      "type": "boolean",
      "default": false
    },
    "altlayout": {
      "type": "boolean",
      "default": false
    },
    "includegradient": {
      "type": "boolean",
      "default": false
    },
    "images": {
      "type": "array",
      "default": []
    },
    "variant": {
      "type": "string",
      "default": "media-text"
    },
    "bgcolour": {
      "type": "string",
      "default": "dark"
    },
    "borderradiustop": {
      "type": "boolean",
      "default": true
    },
    "borderradiusbottom": {
      "type": "boolean",
      "default": true
    },
    "fullwidth": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "media-text",
  "variations": "file:./variations.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": [
    "splide-core",
    "file:./view.js"
  ]
}