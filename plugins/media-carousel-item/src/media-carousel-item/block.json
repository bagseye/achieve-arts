{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bwp/media-carousel-item",
	"version": "0.1.0",
	"title": "Media Carousel Item",
	"category": "widgets",
	"icon": "image-alt2",
	"description": "A single media carousel item. Only available in the Media Carousel block.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"defaultmediaselect": {
			"type": "string",
			"default": "image"
		},
		"defaultimageid": {
			"type": "number",
			"default": null
		},
		"defaultimagesrc": {
			"type": "string",
			"default": ""
