		"clientHeading": {
			"type": "string",
			"default": "Achieve Arts Agency Client"
		},
		"clientName": {
			"type": "string",
			"default": ""
		},
		"projectHeading": {
			"type": "string",
			"default": "Project Name"
		},
		"projectName": {
			"type": "string",
			"default": ""
		},
		"projectTypeHeading": {
			"type": "string",
			"default": "Project Type"
		},
		"projectTypeName": {
			"type": "string",
			"default": ""
		},
		"variant": {
			"type": "string",
			"default": "media-carousel-default"
		}
	},
	"parent": [ "bwp/media-carousel" ],
	"usesContext": [ "bwp/media-carousel-variant" ],
	"textdomain": "media-carousel-item",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
