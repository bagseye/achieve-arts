		"defaultimagealt": {
			"type": "string",
			"default": ""
		},
		"defaultvideoid": {
			"type": "number",
			"default": null
		},
		"defaultvideosrc": {
			"type": "string",
			"default": ""
		},
		"defaultposterid": {
			"type": "number",
			"default": null
		},
		"defaultpostersrc": {
			"type": "string",
			"default": ""
		},
		"defaultposteralt": {
			"type": "string",
			"default": ""
		},
		"videotab": {
			"type": "string",
			"default": ""
